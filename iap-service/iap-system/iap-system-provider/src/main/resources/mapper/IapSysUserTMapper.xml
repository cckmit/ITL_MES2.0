    <!--根据用户名称模糊查询用户信息-->
    <select id="queryUserInfoByUserName" resultType="com.itl.iap.system.api.dto.IapSysUserTDto">
        SELECT
        A.id,
        A.user_name as userName,
        A.user_mobile as userMobile,
        A.user_code as userCode,
        A.real_name as realName,
        A.gender,
        A.avatar,
        A.birthday,
        A.email,
        A.create_date,
        B.position_id as positionId
        FROM
        iap_sys_user_t A
        LEFT  JOIN iap_sys_position_user_t B ON A.id = B.user_id
        <where>
            <if test="username != null and username != ''">
                and A.real_name like concat('%', #{username}, '%')
            </if>
        </where>
        ORDER BY A.create_date ASC
    </select>

    <!-- 根据群ID，查询群成员信息 -->
    <select id="getUserByGroupId" resultMap="groupMemberInfMap">
        SELECT
            U.id,
            U.user_name,
            U.user_mobile,
            U.real_name,
            U.avatar,
            U.gender,
            U.birthday,
            GM.host
        FROM
            iap_sys_user_t AS U
        LEFT JOIN im_group_member AS GM ON U.user_name = GM.account
        LEFT JOIN im_group AS G ON GM.groupId = G.id
        WHERE 1=1
        <if test="groupId != null ">
            AND G.id = #{groupId}
        </if>
    </select>

    <select id="getRoleUserList" resultType="com.itl.iap.system.api.entity.IapSysUserT">

        select a.user_name as userName,a.id,a.real_name as realName
        from iap_sys_user_t a,iap_sys_user_role_t b
        where a.id = b.user_id and b.role_id = #{iapRoleUserQueryDTO.roleId}
        <if test="iapRoleUserQueryDTO.userName!=null">
            and USER_NAME like concat('%',#{iapRoleUserQueryDTO.userName,jdbcType=VARCHAR}, '%')
        </if>
        order by b.last_update_date asc

    </select>

    <select id="getNotRoleUserList" resultType="com.itl.iap.system.api.entity.IapSysUserT">

        select user_name as userName,id,real_name as realName from iap_sys_user_t
        where not exists (
        select 1 from iap_sys_user_role_t
        where iap_sys_user_t.id = iap_sys_user_role_t.user_id
        and iap_sys_user_role_t.role_id = #{iapRoleUserQueryDTO.roleId})
        <if test="iapRoleUserQueryDTO.userName!=null">
            and iap_sys_user_t.user_name like concat('%',#{iapRoleUserQueryDTO.userName,jdbcType=VARCHAR}, '%')
        </if>
        order by iap_sys_user_t.id DESC

    </select>


</mapper>
