        FROM
        iap_sys_user_t A
        LEFT JOIN iap_sys_user_role_t B ON A.id = B.user_id
        LEFT JOIN iap_sys_position_user_t C ON A.id = C.user_id
        LEFT JOIN iap_sys_organization_position_t D ON D.position_id = C.position_id
